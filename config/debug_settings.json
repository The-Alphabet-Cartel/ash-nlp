{
  "debug_settings": {
    "enabled": "${GLOBAL_ENABLE_DEBUG_MODE}",
    "log_level": "${GLOBAL_LOG_LEVEL}",
    "detailed_logging": {
      "environment_variables": true,
      "configuration_substitution": true,
      "model_loading": true,
      "manager_initialization": true,
      "component_status": true
    },
    "performance_logging": {
      "startup_timing": false,
      "model_load_timing": false,
      "request_timing": false
    },
    "development_features": {
      "show_config_values": true,
      "show_cached_configs": true,
      "show_validation_details": true,
      "show_weight_calculations": true
    }
  }
}